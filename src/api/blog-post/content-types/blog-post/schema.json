{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "text": {
      "type": "richtext"
    },
    "EnglishText": {
      "type": "richtext"
    },
    "Title": {
      "type": "string"
    },
    "EnglishTitle": {
      "type": "string"
    },
    "headingImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "minLength": 5,
      "regex": "^[a-z0-9]+(?:-[a-z0-9]+)*$",
      "default": "your-url-text"
    }
  }
}
